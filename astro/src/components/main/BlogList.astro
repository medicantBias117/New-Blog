---
import BlogItem from "./BlogItem.astro";
interface Props {
  category?: string;
  limit?: string;
  pagination?: number;
  isSummary?: boolean;
  sortBy?: string;
}

interface Post {
  userId: number;
  id: number;
  title: string;
  body: string;
}
const { category, limit, pagination, isSummary, sortBy } = Astro.props;
const data = await fetch(
  "https://jsonplaceholder.typicode.com/posts?userId=1"
).then((r) => r.json());
---

{
  data.map((post: Post) => (
    <BlogItem
      userId={post.userId}
      id={post.id}
      title={post.title}
      body={post.body}
    />
  ))
}
